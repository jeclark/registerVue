{
  "name": "12ft-toolkit",
  "version": "1.0.0",
  "description": "12ftguru Vue Starter",
  "author": "Bryan P Johnson <bryan@12ftguru.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "NODE_ENV=production nuxt build",
    "start": "NODE_ENV=production nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "lint-staged",
    "test": "NODE_ENV=test ./node_modules/.bin/jest --coverage",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "browsersList": [
    "> 1%",
    "last 2 versions"
  ],
  "lint-staged": {},
  "jest": {
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js",
      "~/(.*)/([^/]+)$": "<rootDir>/$1/$2"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "mapCoverage": true,
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    }
  },
  "dependencies": {
    "nuxt": "^1.0.0-rc11",
    "postcss-nested": "^3.0.0"
  },
  "devDependencies": {
    "@storybook/vue": "^3.2.13",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.2.0",
    "babel-plugin-groundskeeper-willie": "^1.3.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-declaration-sorter": "^2.1.0",
    "css-mqpacker": "^6.0.1",
    "cssnano": "^3.10.0",
    "eslint": "^4.3.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-vue-preprocessor": "^1.3.1",
    "lint-staged": "^4.3.0",
    "postcss": "^6.0.13",
    "postcss-cli": "^4.1.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-custom-media": "^6.0.0",
    "postcss-import": "^11.0.0",
    "postcss-normalize": "^4.0.0",
    "postcss-prettify": "^0.3.4",
    "postcss-reporter": "^5.0.0",
    "prettier": "^1.7.4",
    "stylelint": "^8.2.0",
    "stylelint-config-recommended": "^1.0.0",
    "vue-test-utils": "^1.0.0-beta.3"
  }
}
